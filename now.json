{
  "version": 2,
  "builds": [{ "src": "server.js", "use": "@now/node-server" }],
  "routes": [
    {
      "src": "/",
      "dest": "/server.js",
      "methods": ["GET"]
    },
    {
      "src": "/products",
      "dest": "/server.js",
      "methods": ["GET"]
    },
    {
      "src": "/products",
      "dest": "/server.js",
      "methods": ["POST"]
    },
    {
      "src": "/products/id/(.*)",
      "dest": "/server.js",
      "methods": ["GET"]
    },
    {
      "src": "/products/(.*)",
      "dest": "/server.js",
      "methods": ["PATCH"]
    },
    {
      "src": "/requestlist",
      "dest": "/server.js",
      "methods": ["GET"]
    },
    {
      "src": "/user",
      "dest": "/server.js",
      "methods": ["GET"]
    }
  ]
}